#ifndef _STLP_STRING
# define _STLP_STRING

#ifndef _STLP_OUTERMOST_HEADER_ID
#  define _STLP_OUTERMOST_HEADER_ID 0x68
#  include <stlport/stl/_prolog.h>
#endif

#ifdef _STLP_PRAGMA_ONCE
#  pragma once
#endif

#if defined (_STLP_USE_OWN_NAMESPACE)
#  include <stlport/stl/_string.h>
#endif

#include <stlport/stl/_string_hash.h>

#if !defined (_STLP_USE_NO_IOSTREAMS)
#  include <stlport/stl/_string_io.h>
#endif

#if (_STLP_OUTERMOST_HEADER_ID == 0x68)
#  include <stlport/stl/_epilog.h>
#  undef _STLP_OUTERMOST_HEADER_ID
#endif

#endif /* _STLP_STRING */

